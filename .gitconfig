[user]
	useConfigOnly = true
[commit]
	gpgsign = true
[core]
	editor = nvim
	excludesfile = ~/.gitignore_global
[color]
	ui = false
[status]
	short = true
	branch = true
[help]
	autocorrect = -1
[format]
	pretty = medium
[push]
	default = current
[credential]
	helper = osxkeychain
[http]
	cookiefile = ~/.gitcookies
