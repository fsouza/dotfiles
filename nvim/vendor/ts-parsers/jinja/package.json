{
  "name": "tree-sitter-jinja",
  "version": "0.0.1",
  "private": true,
  "license": "Apache-2.0",
  "packageManager": "pnpm@9.15.1",
  "workspaces": ["./tree-sitter-jinja", "./tree-sitter-jinja_inline"],
  "scripts": {
    "build": "pnpm build_jinja && pnpm build_jinja_inline",
    "build_jinja": "cd tree-sitter-jinja && pnpm build",
    "build_jinja_inline": "cd tree-sitter-jinja_inline && pnpm build",
    "compile": "pnpm compile_jinja && pnpm compile_jinja_inline",
    "compile_jinja": "cd tree-sitter-jinja && pnpm compile",
    "compile_jinja_inline": "cd tree-sitter-jinja_inline && pnpm compile",
    "test": "pnpm test_jinja && pnpm test_jinja_inline",
    "test_jinja": "cd tree-sitter-jinja && pnpm test",
    "test_jinja_inline": "cd tree-sitter-jinja_inline && pnpm test"
  },
  "engines": {
    "node": ">=v20.12.2"
  },
  "devDependencies": {
    "tree-sitter-cli": "^0.22.6"
  }
}
