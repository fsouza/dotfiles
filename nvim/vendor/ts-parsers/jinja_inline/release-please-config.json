{
  "$schema": "https://raw.githubusercontent.com/googleapis/release-please/main/schemas/config.json",
  "include-v-in-tag": true,
  "include-component-in-tag": true,
  "packages": {
    "tree-sitter-jinja": {
      "release-type": "rust",
      "extra-files": [
        {
          "path": "pyproject.toml",
          "type": "toml",
          "jsonpath": "$.project.version"
        },
        {
          "path": "package.json",
          "type": "json",
          "jsonpath": "$.version"
        },
        {
          "path": "package-lock.json",
          "type": "json",
          "jsonpath": "$.version"
        }
      ]
    },
    "tree-sitter-jinja_inline": {
      "release-type": "rust",
      "extra-files": [
        {
          "path": "pyproject.toml",
          "type": "toml",
          "jsonpath": "$.project.version"
        },
        {
          "path": "package.json",
          "type": "json",
          "jsonpath": "$.version"
        },
        {
          "path": "package-lock.json",
          "type": "json",
          "jsonpath": "$.version"
        }
      ]
    }
  }
}
